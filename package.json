  "description": "Credits management for employment domain",
  "main": "server.js",
  "scripts": {
    "db:up": "docker exec employmentcreditsservice_employment-credits-service_1 sh -c \"\\$(npm bin)/db-migrate up\"",
    "db:down": "docker exec employmentcreditsservice_employment-credits-service_1 sh -c \"\\$(npm bin)/db-migrate down\"",
    "test": "npm run test:stop && docker-compose -f docker-compose.test.yml run service-test && npm run test:stop",
    "test:mocha": "mocha -r bootstrap.test.js 'test/*.test.js'",
    "test:stop": "docker-compose -f docker-compose.test.yml down",
    "test:watch": "mocha -w -G -r bootstrap.test.js **/**/**.test.js",
    "check-build": "check-build",
    "start": "node server.js"
  },
